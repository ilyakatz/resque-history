<% if  start - 20 >= 0 || start + 20 <= size %>
    <p class='pagination'>
      <% if start - 20 >= 0 %>
          <a href="<%= history_path %>?start=<%= start - 20 %>" class='less'>&laquo; less</a>
      <% end %>
      <% if start + 20 < size %>
          <a href="<%= history_path %>?start=<%= start + 20 %>" class='more'>more &raquo;</a>
      <% end %>
    </p>
<% end %>
